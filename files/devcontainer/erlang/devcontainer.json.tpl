{
  "name": "Container Labs erlang",
  "dockerFile": "Dockerfile",
  "extensions": [
    "erlang-ls.erlang-ls",
    "pgourlain.erlang",
    "yuce.erlang-otp",
    "sztheory.erlang-formatter",
    "nigelrook.vscode-linter-erlc",
